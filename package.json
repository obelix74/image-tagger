{
  "name": "image-tagger",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "dev:server": "nodemon --exec ts-node src/index.ts",
    "dev:client": "cd client && npm run dev",
    "dev:both": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "install:all": "npm install && cd client && npm install",
    "build:all": "npm run build && cd client && npm run build",
    "stop": "pkill -f \"ts-node src/index.ts\" && pkill -f \"vite\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "AI-powered image tagger using Gemini API",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@types/uuid": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "exifr": "^7.1.3",
    "express": "^5.1.0",
    "multer": "^2.0.1",
    "sharp": "^0.34.2",
    "sqlite3": "^5.1.7",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.10",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
